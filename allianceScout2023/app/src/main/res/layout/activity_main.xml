        android:hint="Match#"
        android:inputType="number"
        android:maxLength="3"
        android:textColor="@color/white"
        android:textColorHint="@color/gray"
        app:layout_constraintStart_toEndOf="@+id/name_input"
        app:layout_constraintTop_toTopOf="parent" />

